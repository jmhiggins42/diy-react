{
  "name": "diy-react",
  "version": "0.0.2",
  "description": "Basic Typescript React example",
  "main": "dist/public/index.html",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "bash -c 'mkdir dist/public/js' && cp node_modules/react/umd/react.development.js dist/public/js/ && cp node_modules/react-dom/umd/react-dom.development.js dist/public/js/",
    "postbuild": "tsc",
    "clean": "rm -fr dist/app dist/client dist/public/js dist/public/bundle.js dist/public/bundle.js.map",
    "prestart": "npm run build",
    "start": "npm run serve:dev",
    "serve:dev": "./node_modules/.bin/webpack --watch --mode=development",
    "serve:prod": "./node_modules/.bin/webpack --watch --mode=production",
    "test": "npm run build && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmhiggins42/diy-react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "jasmine",
    "webpack"
  ],
  "author": "John Higgins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmhiggins42/diy-react/issues"
  },
  "homepage": "https://github.com/jmhiggins42/diy-react",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.6",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
