{
  "name": "diy-react",
  "version": "0.0.4",
  "description": "Basic Typescript React example",
  "main": "server.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "./scripts/build.sh",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "postbuild": "npm run lint:app; tsc",
    "lint": "npm run lint:app && npm run lint:server",
    "lint:app": "tslint --project tsconfig.json",
    "lint:server": "tslint server.ts webpack.dev.config.ts webpack.prod.config.ts",
    "clean": "./scripts/clean.sh",
    "start": "./scripts/serve.sh",
    "start:dev": "NODE_ENV=development npm run start",
    "start:prod": "NODE_ENV=production npm run start",
    "preserve": "npm run lint:server && tsc --removeComments --noImplicitAny --strict -m commonjs -t es5 --lib dom,es7,es6 server.ts webpack.dev.config.ts webpack.prod.config.ts",
    "serve": "node server.js",
    "serve:dev": "NODE_ENV=development npm run serve",
    "serve:prod": "NODE_ENV=production npm run serve",
    "test": "npm run build && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmhiggins42/diy-react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "jasmine",
    "webpack"
  ],
  "author": "John Higgins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmhiggins42/diy-react/issues"
  },
  "homepage": "https://github.com/jmhiggins42/diy-react",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/webpack": "^4.4.23",
    "@types/webpack-dev-server": "^3.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
