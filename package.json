{
  "name": "diy-react",
  "version": "0.0.3",
  "description": "Basic Typescript React example",
  "main": "dist/public/index.html",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "./scripts/build.sh",
    "build:dev": "NODE_ENV=development npm run build",
    "build:prod": "NODE_ENV=production npm run build",
    "postbuild": "tsc",
    "clean": "./scripts/clean.sh",
    "start": "./scripts/serve.sh",
    "start:dev": "NODE_ENV=development npm run start",
    "start:prod": "NODE_ENV=production npm run start",
    "serve:dev": "webpack-dev-server --open --mode=development",
    "serve:prod": "webpack-dev-server --open --mode=production",
    "test": "npm run build && jasmine"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jmhiggins42/diy-react.git"
  },
  "keywords": [
    "react",
    "typescript",
    "jasmine",
    "webpack"
  ],
  "author": "John Higgins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmhiggins42/diy-react/issues"
  },
  "homepage": "https://github.com/jmhiggins42/diy-react",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
